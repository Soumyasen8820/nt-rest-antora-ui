<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width,initial-scale=1">
		<title>{{{detag (or page.title defaultPageTitle)}}}{{#with site.title}} :: {{this}}{{/with}}</title>
		{{#with page.canonicalUrl}}
		<link rel="canonical" href="{{{this}}}">
		{{/with}}
		{{#with (or antoraVersion site.antoraVersion)}}
		<meta name="generator" content="Antora {{this}}">
		{{/with}}
		<link rel="stylesheet" href="{{{uiRootPath}}}/css/antora.css">
		<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet"> 
		<link href="https://fonts.googleapis.com/css2?family=Roboto+Mono:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet"> 
	</head>
	<body class="hlmrf article{{#with (or page.attributes.role page.role)}} {{{this}}}{{/with}}">
{{> header}}
{{> lside}}
{{> main}}
{{> rside}}
{{> footer}}
	</body>
</html>
